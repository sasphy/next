{
  "name": "music-streaming",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest run --config vitest.config.mts",
    "test:watch": "vitest --config vitest.config.mts",
    "test:ui": "vitest --ui --config vitest.config.mts",
    "test:coverage": "vitest run --coverage --config vitest.config.mts",
    "dev:with-api": "bun run ../music-streaming-server/src/index.ts & next dev",
    "dev:test-api": "bun run ../music-streaming-server/src/tests/test-server.ts & next dev",
    "test:all": "cd ../music-streaming-server && bun test && cd ../music-streaming && bun test",
    "start:all": "bun run test:all && (cd ../music-streaming-server && bun run start:test) & next dev"
  },
